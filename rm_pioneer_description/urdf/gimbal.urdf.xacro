<?xml version="1.0" encoding="utf-8"?>

<robot name="gimbal"
  xmlns:xacro="http://ros.org/wiki/xacro">

  <link name="base_link"/>

  <link name="yaw_link">
    <inertial>
      <origin xyz="0.000383855771423802 0.0331265690847331 0.0935981552484009" rpy="0 0 0" />
      <mass value="0.709953915948123" />
      <inertia ixx="0.000574881362328483" ixy="-2.62346178707535E-06" ixz="3.78018226777921E-07" iyy="0.000515386950029736" iyz="8.98732079277232E-07" izz="0.000826646711110034" />
